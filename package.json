{
  "name": "remark-mdx-remove-esm",
  "version": "1.0.1",
  "description": "Remark plugin to remove import and/or export statements (mdxjsEsm)",
  "type": "module",
  "exports": "./dist/esm/index.js",
  "main": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run prebuild && tsc",
    "lint": "eslint .",
    "prettier": "prettier --write .",
    "test": "vitest",
    "test:file": "vitest without_options.spec.ts",
    "test:ci": "vitest --watch=false",
    "test:type": "type-coverage",
    "prepack": "npm run build",
    "prepublishOnly": "npm run test:ci && type-coverage &&npm run prettier && npm run lint"
  },
  "files": [
    "dist/",
    "src/",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipikuka/remark-mdx-remove-esm.git"
  },
  "keywords": [
    "unified",
    "mdast",
    "remark",
    "markdown",
    "MDX",
    "plugin",
    "remark-plugin",
    "ESM",
    "mdxjsEsm",
    "mdx-remove-esm",
    "mdx-remove-mdxjsEsm"
  ],
  "author": "ipikuka <talatkuyuk@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ipikuka/remark-mdx-remove-esm#readme",
  "bugs": {
    "url": "https://github.com/ipikuka/remark-mdx-remove-esm/issues"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^3.0.1",
    "@types/dedent": "^0.7.2",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "dedent": "^0.7.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "prettier": "^3.1.1",
    "rehype-format": "^5.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-gfm": "^4.0.0",
    "remark-mdx": "^3.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "rimraf": "^5.0.5",
    "type-coverage": "^2.27.1",
    "typescript": "^5.3.3",
    "unified": "^11.0.4",
    "vitest": "^1.3.0"
  },
  "dependencies": {
    "@types/mdast": "^4.0.0",
    "mdast-util-mdxjs-esm": "^2.0.0",
    "unist-util-remove": "^4.0.0"
  },
  "typeCoverage": {
    "atLeast": 100,
    "detail": true,
    "ignoreCatch": true,
    "strict": true
  },
  "sideEffects": false
}
